const invalidChars = /['\t\b\r()"]/g;
const charMap = getCharMap(lang);

function getLangMap(lang) {
	for(let i = 0; i < langMap.length; i++) {
		if(langMap[i].language === lang) {
			return langMap[i];
		}
	}
}

function getCharMap(lang) {
	let charMapDefault = getLangMap("default").characterMap;
	let charMapSpecific = null;
	let langMapSpecific = getLangMap(lang);
	if (langMapSpecific) {
		charMapSpecific = langMapSpecific.characterMap;
	}
	if (charMapDefault && charMapSpecific) {
		for (i=0; i<charMapDefault.length; i++) {
			for (j=0; j<charMapSpecific.length; j++) {
				if (charMapDefault[i].accentedCharacter === charMapSpecific[j].accentedCharacter) {
					charMapDefault[i].normalCharacter = charMapSpecific[j].normalCharacter;
				}
			}
		}
	}
	return charMapDefault;
}

function convertAccented(field) {
	let text = field.value;
	for(i = 0; i < charMap.length; i++) {
		text = text.split(charMap[i].accentedCharacter).join(charMap[i].normalCharacter);
	}
	field.value = text;
}

function convertAccented(field, infoTextId) {
	let text = field.value;
	let infoText = document.getElementById(infoTextId);

	for(let i = 0; i < charMap.length; i++) {
		text = text.split(charMap[i].accentedCharacter).join(charMap[i].normalCharacter);
	}

	if(field.value != text && infoText != null) {
		infoText.style.display="block";
	}
	else if(infoText != null) {
		infoText.style.display="none";
	}

	field.value = text.replace(invalidChars, "");
}
